// Approach https://www.youtube.com/watch?v=32Ll35mhWg0&list=PLgUwDviBIf0rPG3Ictpu74YWBQ1CaBkm2&index=2
func findDuplicate(nums []int) int {
    slow:=nums[0]
    fast:=nums[0]
    for {
        slow=nums[slow]
        fast=nums[nums[fast]]

        if slow==fast {
            break
        }
    }

    fast=nums[0]
    for fast!=slow{
        slow=nums[slow]
        fast=nums[fast]
    }
    return slow
}
