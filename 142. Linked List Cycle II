/**
 * Definition for singly-linked list.
 * type ListNode struct {
 *     Val int
 *     Next *ListNode
 * }
 */
func detectCycle(head *ListNode) *ListNode {
    fast,slow:=head,head
    for true {
        if fast == nil || slow == nil {
            return nil
        }

        if fast.Next == nil {
            return nil
        }

        fast=fast.Next.Next
        slow = slow.Next
        if fast == slow {
            break
        }
    }
    // Collision already found

    fast = head
    for fast != slow {
        slow = slow.Next
        fast=fast.Next
    }

    return slow
}
